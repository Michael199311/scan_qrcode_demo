#import "SQTboxListTableViewController.h"
#import "SQTboxCell.h"
#import "SQModelTboxInfo.h"

@interface SQTboxListTableViewController ()

@end

@implementation SQTboxListTableViewController

- (void)viewDidLoad {
    [super viewDidLoad];
    self.navigationItem.title = @"Tbox列表";
    [self.tableView registerNib:[UINib nibWithNibName:@"SQTboxCell" bundle:[NSBundle mainBundle]] forCellReuseIdentifier:@"Cell"];
    
}

- (void)didReceiveMemoryWarning {
    [super didReceiveMemoryWarning];
}

#pragma mark - Table view data source

- (NSInteger)numberOfSectionsInTableView:(UITableView *)tableView {
    return 1;
}

- (NSInteger)tableView:(UITableView *)tableView numberOfRowsInSection:(NSInteger)section {
    return self.tboxArr.count;
}


- (UITableViewCell *)tableView:(UITableView *)tableView cellForRowAtIndexPath:(NSIndexPath *)indexPath {
    SQTboxCell *cell = [tableView dequeueReusableCellWithIdentifier:@"Cell"];
    SQModelTboxInfo *tbox = self.tboxArr[indexPath.row];
    cell.TboxidLabel.text = tbox.TboxId;
    cell.typeLabel.text = tbox.tcuModelId;
    cell.iccidLabel.text = tbox.iccid;
    cell.createTimeLabel.text = tbox.createdTime;
    cell.locationLabel.text = tbox.locationName;
    cell.statusLabel.text = tbox.statusValue;
    
    return cell;
}



- (BOOL)tableView:(UITableView *)tableView canEditRowAtIndexPath:(NSIndexPath *)indexPath {
    // Return NO if you do not want the specified item to be editable.
    return YES;
}

- (CGFloat)tableView:(UITableView *)tableView heightForRowAtIndexPath:(NSIndexPath *)indexPath{
    return 176;
}


